# Altera aliases and license file setting.
#
# Depends on install location and version.
# No arguments required

# Altera tools
alias quartus='/opt/altera/12.1/quartus/bin/quartus --64bit '
alias qpgm='/opt/altera/12.1/quartus/bin/quartus_pgmw '
alias nios2='/opt/altera/12.1/nios2eds/nios2_command_shell.sh '
alias eclipse='/opt/altera/12.1/nios2eds/bin/eclipse-nios2 '
export LM_LICENSE_FILE=3333@gol-emis-license.kstg.corp
